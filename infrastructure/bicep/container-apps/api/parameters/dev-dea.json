{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "containerImage": {
      "value": "dfxswiss/deuro-api:beta"
    },
    "containerCPU": {
      "value": "0.5"
    },
    "containerMemory": {
      "value": "1Gi"
    },
    "containerEnvPort": {
      "value": "3000"
    },
    "containerEnvConfigProfile": {
      "value": "localhostMainnet"
    },
    "containerEnvConfigChain": {
      "value": "ethereum"
    },
    "containerEnvConfigAppUrl": {
      "value": "https://dev.app.deuro.com"
    },
    "containerEnvConfigIndexerUrl": {
      "value": "https://dev.ponder.deuro.com"
    },
    "containerEnvCoingeckoApiKey": {
      "value": "[API-KEY]"
    },
    "containerEnvTelegramBotToken": {
      "value": "[API-KEY]"
    },
    "containerEnvRpcUrlMainnet": {
      "value": "https://eth-mainnet.g.alchemy.com/v2/[API-KEY]"
    },
    "containerEnvRpcUrlPolygon": {
      "value": "https://polygon-mainnet.g.alchemy.com/v2/[API-KEY]"
    }
  }
